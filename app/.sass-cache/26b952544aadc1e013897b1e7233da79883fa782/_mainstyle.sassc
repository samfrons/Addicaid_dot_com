$experimental-support-for-mozilla : true !default
$experimental-support-for-webkit : true !default
$support-for-original-webkit-gradients : true !default
$experimental-support-for-opera : true !default
$experimental-support-for-microsoft : true !default

$main-color: #2A2a2a
$main: #fff
$bordercolor: rgba(15,32,36,1)
$topcolor: rgba(175,202,206,0.2)
$bottomcolor: rgba(101,142,146,0.5)
$innercolor: rgba(91,62,86,0.7)
$teel: #00c6d4
$teel_light: #00c6d4

@font-face
 font-family: "gotham"
 src: url('../../fonts/Gotham-Book.otf') format('opentype')

@mixin backgrounds
 background-color: transparent
 border-style: none
 min-height: 20px
 background-repeat: no-repeat
 background-size: 80% auto

@mixin calendar
 border-top: 10px solid black
 border-left: 1px solid black
 border-right: 1px solid black
 border-bottom: 1px solid black
 border-radius: 5px
 text-align: center
 width: 50px
 height: 30px

$topcolor: rgba(249,47,5,1)
$bottomcolor: rgba(219,87,5,1)
$shadowtop: rgba(219,31,5,1)
$shadowbottom: rgba(0,0,0,.7)
$buttonbackground: rgba(219,87,5,1)


@mixin gradientbutton
 background: #e3e3e3
 border-radius: 3px
 -moz-box-shadow: 0px 5px 0px $shadowtop, 0px 9px 25px $shadowbottom
 box-shadow: 0px 5px 0px $shadowtop, 0px 9px 25px $shadowbottom
 background-color: $buttonbackground
 background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0, $topcolor), color-stop(1, $bottomcolor) )

@mixin gradientaa
 $bottomcolor: rgba(9,157,211,1)
 $topcolor: rgba(9,107,131,1)
 $shadowtop: rgba(9,71,101,1)
 $shadowbottom: rgba(0,0,0,.7)
 $buttonbackground: rgba(19,7,5,1)
 border-radius: 3px
 -moz-box-shadow: 0px 5px 0px $shadowtop, 0px 9px 25px $shadowbottom
 box-shadow: 0px 5px 0px $shadowtop, 0px 9px 25px $shadowbottom
 background-color: $buttonbackground
 background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0, $topcolor), color-stop(1, $bottomcolor) )

@mixin gradientna
 $bottomcolor: rgba(255,3,5,1)
 $topcolor: rgba(180,3,5,1)
 $shadowtop: rgba(129,1,5,1)
 $shadowbottom: rgba(0,0,0,.7)
 $buttonbackground: rgba(219,87,5,1)
 border-radius: 3px
 -moz-box-shadow: 0px 5px 0px $shadowtop, 0px 9px 25px $shadowbottom
 box-shadow: 0px 5px 0px $shadowtop, 0px 9px 25px $shadowbottom
 background-color: $buttonbackground
 background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0, $topcolor), color-stop(1, $bottomcolor) )

@mixin gradientbutton_active
 -webkit-box-shadow: 0px 3px 0px $shadowtop, 0px 3px 6px $shadowbottom
 -moz-box-shadow: 0px 3px 0px $shadowtop, 0px 3px 6px $shadowbottom
 box-shadow: 0px 3px 0px $shadowtop, 0px 3px 6px $shadowbottom
 position: relative
 top: 6px

@mixin gradientbutton_lime
 $bottomcolor: rgba(225,243,85,1)
 $topcolor: rgba(165,163,75,1)
 $shadowtop: rgba(125,123,45,1)
 $shadowbottom: rgba(0,0,0,.7)
 $buttonbackground: rgba(165,163,75,1)
 border-radius: 5px
 background-image: -webkit-gradient( linear, left bottom, left top, color-stop(0, $topcolor), color-stop(1, $bottomcolor) )
 -webkit-box-shadow: 0px 3px 0px $shadowtop, 0px 3px 6px $shadowbottom
 -moz-box-shadow: 0px 3px 0px $shadowtop, 0px 3px 6px $shadowbottom
 box-shadow: 0px 3px 0px $shadowtop, 0px 3px 6px $shadowbottom

@mixin gradientlight
 $bottomcolor: rgba(237,232,231,0.2)
 $topcolor: rgba(237,202,221,0.0)
 $innercolor: rgba(187,142,21,0.2)
 background-image: -webkit-linear-gradient(top, $topcolor 0%, $bottomcolor 100%)

